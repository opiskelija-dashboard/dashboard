{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://opiskelija-dashboard.github.io/dashboard/",
  "dependencies": {
    "axios": "^0.16.2",
    "chart.js": "^2.6.0",
    "gh-pages": "^1.0.0",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.1",
    "react-chartjs-2": "^2.5.5",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-scripts": "1.0.10",
    "redux": "^3.7.2",
    "redux-promise": "^0.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "testRS": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "ava --verbose",
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-env": "^3.1.0",
    "coveralls": "^2.13.1",
    "enzyme": "^2.9.1",
    "ignore-styles": "^5.0.1",
    "react-addons-test-utils": "^15.6.0",
    "redux-ava": "^2.2.0",
    "redux-mock-store": "latest",
    "sinon": "^2.3.8"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "ava": {
    "files": [
      "src/*/test/*.js",
      "!**/not-this-file.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "require": [
      "babel-register",
      "ignore-styles",
      "./src/test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  }
}
